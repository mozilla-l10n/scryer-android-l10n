#. Label used for buttons to create new collection, e.g. in dialogs
msgctxt "action_create"
msgid "Create new collection"
msgstr "Buat koleksi baru"

#. Label used for buttons to delete items, e.g. in dialogs
msgctxt "action_delete"
msgid "Delete"
msgstr "Hapus"

#. Label used for title in screenshot info dialogue, but for action in menu
msgctxt "info_info"
msgid "Screenshot info"
msgstr "Info cuplikan layar"

#. Label used when no content
msgctxt "title_noshots"
msgid "No screenshots yet!"
msgstr "Belum ada cuplikan layar!"

#. Label used for buttons to skip the current action
msgctxt "action_later"
msgid "Later"
msgstr ""

#. Notification: strings displayed on the notification to perform various
#. actions
msgctxt "notification_action_capture"
msgid "Tap to take screenshot"
msgstr "Ketuk untuk mengambil cuplikan layar"

msgctxt "notification_action_open"
msgid "Open app"
msgstr "Buka aplikasi"

msgctxt "notification_action_stop"
msgid "Stop"
msgstr "Hentikan"

#. String displayed to inform users new screenshots captured
msgctxt "notification_action_collect_title"
msgid "New screenshots captured"
msgstr "Cuplikan layar baru"

msgctxt "notification_action_collect"
msgid "Tap to sort your screenshots"
msgstr "Ketuk untuk mengelompokkan cuplikan layar"

#. Sorting panel: strings displayed on the sorting panel to help users
#. categorize screenshots,      Refer to UX spec 1.1:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133909_1-1_Key_Flow_Add_To_Collection
#. String displayed to skip the current screenshot and move on to the next one
msgctxt "multisorting_action_next"
msgid "Next"
msgstr "Selanjutnya"

#. String displayed on the title of sorting panel and the screenshot will be
#. categorized to a collection
msgctxt "sorting_title_move"
msgid "Move to"
msgstr "Pindahkan ke"

#. Strings displayed to suggest default collections for users to sort
msgctxt "sorting_suggestion_1st"
msgid "Shopping"
msgstr "Belanja"

msgctxt "sorting_suggestion_2nd"
msgid "Finance & Banking"
msgstr "Finansial & Perbankan"

msgctxt "sorting_suggestion_3rd"
msgid "News & Article"
msgstr "Berita & Artikel"

msgctxt "sorting_suggestion_4th"
msgid "Chat history"
msgstr "Riwayat percakapan"

msgctxt "sorting_suggestion_5th"
msgid "Event & Activity"
msgstr "Acara & Aktivitas"

#. Strings displayed to allow users to create collection name
msgctxt "dialogue_title_collection"
msgid "Collection name"
msgstr "Nama koleksi"

#. String displayed as a hint to ask users to type a name
msgctxt "dialogue_inputfield_hint"
msgid "Give me a name"
msgstr "Beri nama"

#. String displayed to create a new collection and add the screenshot into it
msgctxt "dialogue_action_add"
msgid "Add"
msgstr "Tambah"

#. Home view: strings displayed on the home view,     Refer to UX spec 2.1:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133914_2-1_Navigation
msgctxt "home_action_search"
msgid "Search"
msgstr "Cari"

msgctxt "menu_home_action_settings"
msgid "Settings"
msgstr "Setelan"

msgctxt "home_separator_access"
msgid "Quick access"
msgstr "Akses cepat"

msgctxt "home_action_more"
msgid "View all"
msgstr "Lihat semua"

msgctxt "home_separator_collection"
msgid "Collections"
msgstr "Koleksi"

#. String displayed to allow users to enter a collection with all unsorted
#. screenshots
msgctxt "home_action_unsorted"
msgid "Unsorted"
msgstr "Belum Disortir"

#. Collection view: string displayed on the collection view. Parameter %1$d is
#. the quantity of screenshot files, i.e. 100 shots
#, c-format
msgctxt "collection_separator_shots"
msgid "%1$d shots"
msgstr "%1$d cuplikan"

#. String displayed to inform users all screenshots are selected
msgctxt "collection_header_select_all"
msgid "All selected"
msgstr "Semua yang dipilih"

#. String displayed to inform users to select screenshot
msgctxt "collection_header_select_none"
msgid "Select screenshots"
msgstr "Pilih cuplikan layar"

msgctxt "collection_header_viewall_all"
msgid "All screenshots"
msgstr "Semua cuplikan layar"

#. String displayed when text recognition has an unknown error
msgctxt "detail_ocr_error_failed"
msgid "Text recognition failed"
msgstr "Gagal mengenali teks"

#. String displayed when text recognition fails because the image is too big
#. or has unacceptable proportion
msgctxt "detail_ocr_error_edgecase"
msgid "Something went wrong? Crop the screenshot and retry."
msgstr "Ada yang salah? Potong cuplikan layar dan coba kembali."

#. String displayed when recognizing no text on the screenshot
msgctxt "detail_ocr_error_notext"
msgid "No text found"
msgstr "Teks tidak ditemukan"

#. Detail view: string displayed on the detail view when recognizing the text
#. on the screenshot
#, fuzzy
msgctxt "detail_transition_recognizing"
msgid "Recognizing text…"
msgstr "Mengenali teks…"

#. Dialogue: strings displayed in the dialogues which inform users about a
#. task, e.g. dialogue title, content and action
#. This string also serves as menu action in collection view
msgctxt "dialogue_collecitioninfo_title_info"
msgid "Collection info"
msgstr "Info koleksi"

#. String displayed as a tag to accomodate the name attribute, e.g. Name:
#. shopping
msgctxt "dialogue_list_name"
msgid "Name"
msgstr "Nama"

msgctxt "dialogue_collecitioninfo_list_shot"
msgid "Total screenshots"
msgstr "Jumlah cuplikan layar"

msgctxt "dialogue_collecitioninfo_list_storage"
msgid "Storage used"
msgstr "Penyimpanan terpakai"

msgctxt "dialogue_shotinfo_list_size"
msgid "Size"
msgstr "Ukuran"

msgctxt "dialogue_list_edit"
msgid "Last edit"
msgstr "Terakhir diubah"

msgctxt "dialogue_deletecollection_title_delete"
msgid "Delete collection?"
msgstr "Hapus koleksi?"

msgctxt "dialogue_deleteshot_title_delete"
msgid "Delete screenshot?"
msgstr "Hapus cuplikan layar?"

#. String displayed to state that the action will erase "all screenshots in
#. the collection," which is "its screenshots" in the string.
msgctxt "dialogue_delete_content_cantundo"
msgid ""
"Are you sure? Deleting this collection will permanently remove all of its"
" screenshots from your phone."
msgstr ""
"Anda yakin? Menghapus koleksi ini akan menghapus semua cuplikan layar "
"dari ponsel Anda secara permanen."

#. String displayed to state that the action will erase all selected
#. screenshots.
msgctxt "dialogue_deleteshot_content_delete"
msgid ""
"Are you sure? Deleting screenshots will permanently remove screenshots "
"from your phone."
msgstr ""
"Anda yakin? Menghapus cuplikan layar ini akan menghapus cuplikan layar "
"dari ponsel Anda secara permanen."

#. Parameter %1$d is the quantity of screenshot files, i.e. 100 screenshots
#, c-format
msgctxt "dialogue_deletecollection_content_shots"
msgid "%1$d screenshots"
msgstr "%1$d cuplikan layar"

msgctxt "dialogue_rename_title_rename"
msgid "Rename collection"
msgstr "Ubah nama koleksi"

msgctxt "dialogue_rename_error_duplicate"
msgid "There is already a collection with this name"
msgstr "Sudah ada koleksi dengan nama ini"

msgctxt "dialogue_rename_error_maximum"
msgid "20 characters maximum"
msgstr "Maksimal 20 karakter"

#. Dialogue: Strings displayed when users skip multi-sorting panel,     Refer
#. to UX spec 2.4_E:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133917_2-4_Detect_New_Screenshots
msgctxt "dialogue_skipsorting_title_skip"
msgid "Skip the rest?"
msgstr "Lewati sisanya?"

msgctxt "dialogue_skipsorting_content_moveto"
msgid "Skip remaining screenshots and move to “Unsorted?”"
msgstr "Lewati cuplikan layar lainnya dan pindahkan ke kelompok “Belum Disortir?”"

#. Parameter %1$d is the quantity of screenshot files, i.e. 100 remaining
#, c-format
msgctxt "dialogue_skipsorting_content_count"
msgid "%1$d remaining"
msgstr "%1$d lagi"

#. String to allow users to skip sorting the remaining screenshots
msgctxt "dialogue_skipsorting_action_skip"
msgid "Skip all"
msgstr "Lewati semua"

#. Menu: strings displayed when triggering the overflow menu in the top-right
#. corner of each view
msgctxt "menu_shot_action_move"
msgid "Move to…"
msgstr "Pindahkan ke…"

msgctxt "menu_action_rename"
msgid "Rename"
msgstr "Ubah nama"

#. Strings displayed to perform actions when selecting text on the screenshot,
#. Refer to UX spec 4.2_G&I:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133922_4-2_Image_-_Text_Mode
msgctxt "menu_ocr_action_web"
msgid "Web search"
msgstr "Pencarian web"

msgctxt "menu_ocr_action_copy"
msgid "Copy"
msgstr "Salin"

msgctxt "menu_action_selectall"
msgid "Select all"
msgstr "Pilih semua"

msgctxt "menu_action_share"
msgid "Share"
msgstr "Bagikan"

#. Bottom sheet: strings displayed on the reminder panel, and the placeholder
#. %1$s is the app name, ScreenshotGo
#, c-format
msgctxt "sheet_enable_title_enable"
msgid "Enable %1$s to take screenshots?"
msgstr "Aktifkan %1$s untuk mencuplik layar?"

msgctxt "sheet_enable_content_enable"
msgid "You won’t be able to take screenshots with one tap unless this is enabled."
msgstr ""
"Anda tidak dapat mencuplik layar dengan sekali ketuk kecuali ini "
"diaktifkan."

#. String displayed to allow users to enable the screenshot service
msgctxt "sheet_enable_action_enable"
msgid "Enable"
msgstr "Aktif"

msgctxt "sheet_unsorted_title_unsorted"
msgid "Unsorted screenshots"
msgstr "Cuplikan Layar Belum Disortir"

#. Parameter %1$d is the quantity of screenshot files, i.e. 100 unsorted
#. screenshots. Sort them now?
#, c-format
msgctxt "sheet_unsorted_content_shots"
msgid "%1$d unsorted screenshots. Sort them now?"
msgstr "%1$d cuplikan layar yang belum disortir. Sortir sekarang?"

msgctxt "sheet_box_ask"
msgid "Don’t ask again"
msgstr "Jangan tanya lagi"

msgctxt "sheet_unsorted_action_sort"
msgid "Sort"
msgstr "Sortir"

msgctxt "sheet_action_no"
msgid "No thanks"
msgstr "Tidak, terima kasih"

#. Snackbar: strings to provide confirmations for user’s actions
msgctxt "snackbar_disable"
msgid "Screenshot feature disabled"
msgstr "Layanan pencuplik layar dinonaktifkan"

msgctxt "snackbar_enable"
msgid "Screenshot feature enabled"
msgstr "Layanan pencuplik layar diaktifkan"

#. Parameter %1$s is the collection name user defined, i.e. Shopping
#, c-format
msgctxt "snackbar_addto"
msgid "Added to %1$s"
msgstr "Ditambahkan ke %1$s"

msgctxt "snackbar_copied"
msgid "Copied"
msgstr "Disalin"

#. Settings: strings displayed in the settings page,     Refer to UX spec 6.1:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133927_6-1_Settings
#. the string displayed to inform users this switch needs to be enabled to
#. take screenshots. If enabled, the system reveals two more options for users
#. to customize
msgctxt "settings_list_capture"
msgid "Enable screenshot feature"
msgstr "Aktifkan fitur mencuplik layar"

#. The string changes to serve as description of settings_list_fab
msgctxt "settings_detail_capture"
msgid ""
"Allows you to take a screenshot with one tap. Works with whatever is on "
"your screen."
msgstr ""
"Memungkinkan Anda mencuplik layar dengan sekali ketuk. Bekerja dengan apa"
" pun yang ada di layar Anda."

#. The string displayed to enable a floating button to take screenshot, and
#. the placeholder %1$s is "GO" from the product name
#, c-format, fuzzy
msgctxt "settings_list_fab"
msgid "Use %1$s button"
msgstr "Tombol %1$s"

msgctxt "settings_list_sorting"
msgid "Prompt to sort"
msgstr "Tanyakan untuk menyortir"

msgctxt "settings_detail_sorting"
msgid "Always prompt to add screenshot to a collection"
msgstr "Selalu tanyakan untuk menambah cuplikan layar ke koleksi"

msgctxt "settings_list_usagedata"
msgid "Send usage data"
msgstr "Kirim data penggunaan"

#. Parameter %1$s is the app name, i.e. Firefox ScreenshotGo
#, c-format
msgctxt "settings_detail_mozilla"
msgid ""
"Mozilla strives to collect only what we need to provide and improve %1$s "
"for everyone."
msgstr ""
"Mozilla berusaha untuk mengumpulkan hanya apa yang kami perlu untuk "
"sediakan dan meningkatkan %1$s untuk setiap orang."

msgctxt "settings_action_learnmore"
msgid "Learn more"
msgstr "Pelajari lebih lanjut"

msgctxt "settings_list_feedback"
msgid "Give feedback"
msgstr "Kirim saran"

msgctxt "feedback_title_lovefeedback"
msgid "We love feedback!"
msgstr "Kami menyukai saran!"

msgctxt "feedback_detail_letusknow"
msgid "Let us know what you think."
msgstr "Beritahu kami pendapat Anda."

msgctxt "feedback_action_rate"
msgid "Yes, rate 5 stars!"
msgstr "Ya, beri 5 bintang!"

msgctxt "feedback_action_feedback"
msgid "No, send feedback."
msgstr "Tidak, kirim saran."

msgctxt "settings_list_share"
msgid "Share with friends"
msgstr "Bagikan dengan teman"

#. The strings displys when users share the app with friends. %1$s is the app
#. name i.e. Firefox ScreenshotGo, and %2$s is the Google Play url
#, c-format
msgctxt "share_intro"
msgid ""
"Hey, check out this new app that helps you organize and find your "
"screenshots easily. It’s called %1$s. %2$s"
msgstr ""
"Hei, lihat aplikasi baru ini yang membantu Anda mengelola dan menemukan "
"cuplikan layar Anda dengan mudah. Aplikasi ini bernama %1$s. %2$s"

msgctxt "settings_list_about"
msgid "About"
msgstr "Tentang"

#. Parameter %1$s is version code of the app, i.e. 1.0.0
#, c-format
msgctxt "about_content_version"
msgid "Version %1$s"
msgstr "Versi %1$s"

msgctxt "about_list_support"
msgid "Support"
msgstr "Dukungan"

msgctxt "about_list_right"
msgid "Your Rights"
msgstr "Hak Anda"

msgctxt "about_list_privacy"
msgid "Privacy Notice"
msgstr "Kebijakan Privasi"

#. Onboarding: strings to onboard users to grant permissions,     Refer to UX
#. spec 7.1:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133929_7-1_Onboarding
#. Parameter %1$s is the app name, i.e. Firefox ScreenshotGo
#, c-format
msgctxt "onboarding_storage_title_welcome"
msgid "Welcome to %1$s"
msgstr "Selamat datang di %1$s"

msgctxt "onboarding_storage_content_permission"
msgid ""
"To help you organize and find your screenshots better, we’ll need your "
"permission to access your storage."
msgstr ""
"Untuk membantu untuk kelola dan temukan cuplikan layar Anda dengan lebih "
"baik, kami memerlukan izin Anda untuk mengakses penyimpanan."

#. The string displays if the system autogrants storage permission. Parameter
#. %1$s is the app name, i.e. Firefox ScreenshotGo
#, c-format
msgctxt "onboarding_storage_content_autogrant"
msgid "%1$s helps you organize and find your screenshots better."
msgstr ""
"%1$s bantu Anda mengelola dan menemukan cuplikan layar Anda dengan lebih "
"baik."

msgctxt "onboarding_transition_loading"
msgid "Loading"
msgstr "Memuat"

#. Overlay permission prompt: strings to onboard users to grant permissions,
#. and the placeholder %1$s is "GO" from the product name
#, c-format
msgctxt "onboarding_fab_title_fab"
msgid "Get the %1$s button"
msgstr "Dapatkan tombol %1$s"

#. The placeholder %1$s is "GO" from the product name
#, c-format
msgctxt "onboarding_fab_content_permission"
msgid ""
"The %1$s button lets you take and organize your screenshots anytime, "
"anywhere. We’ll need your permission to enable it."
msgstr ""
"Tombol %1$s memungkinkan Anda mengambil dan mengelola cuplikan layar "
"kapanpun, di manapun. Kami membutuhkan izin Anda untuk mengaktifkan ini."

#. Auto-grant overlay permission prompt. The placeholder %1$s is "GO" from the
#. product name
#, c-format
msgctxt "onboarding_autogrant_overlay_title"
msgid "Use the %1$s button to take and organize screenshots anytime, anywhere."
msgstr ""
"Gunakan tombol %1$s untuk mengambil dan mengelola cuplikan layar "
"kapanpun, di mana pun."

msgctxt "onboarding_autogrant_overlay_gotit"
msgid "Got it"
msgstr "Mengerti"

#. Onboarding: strings to inform users when keeping denying permission
msgctxt "onboarding_error_title_wrong"
msgid "Oops! Something’s wrong"
msgstr "Ups! Ada masalah"

#. Parameter %1$s is the app name, i.e. Firefox ScreenshotGo
#, c-format
msgctxt "onboarding_error_content_permission"
msgid "%1$s needs this permission to work correctly."
msgstr "%1$s memerlukan izin ini untuk bekerja dengan baik."

msgctxt "onboarding_error_action_allow"
msgid "Allow permission"
msgstr "Izinkan"

#. Parameter %1$s is the app name, i.e. Firefox ScreenshotGo
#, c-format
msgctxt "onboarding_rareerror_content_permission"
msgid ""
"%1$s needs permission to access your phone’s storage. Please find "
"Permission to turn on Storage."
msgstr ""
"%1$s membutuhkan izin untuk mengakses penyimpanan ponsel Anda. Temukan di"
" bagian Izin untuk mengaktifkan Penyimpanan."

msgctxt "onboarding_rareerror_action_goto"
msgid "Go to permission"
msgstr "Ke bagian Izin"

#. Onboarding: strings to inform users that the search feature is WIP,
#. Refer to UX spec 7.3:
#. https://mozilla.invisionapp.com/share/24N3MMQB986#/310133930_7-3_Other_Oborading
msgctxt "onboarding_search_title_soon"
msgid "Search is coming soon!"
msgstr "Pencarian akan segera hadir!"

#. The string displayed to describe the survey
msgctxt "onboarding_search_content_survey"
msgid ""
"We’re working on a magic feature to help you search screenshots with "
"text. Interested?"
msgstr ""
"Kami sedang mengerjakan fitur canggih untuk membantu Anda mencari "
"cuplikan layar berdasarkan teksnya. Tertarik?"

msgctxt "onboarding_search_action_agree"
msgid "Yes please"
msgstr "Ya"

#. The string should represent an informal variant of no
msgctxt "onboarding_search_action_disagree"
msgid "Nope"
msgstr "Tidak"

msgctxt "onboarding_search_content_feedback"
msgid "Thanks for your feedback!"
msgstr "Terima kasih atas umpan balik Anda!"

